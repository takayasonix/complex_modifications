{
  "title": "home // fnを押しながらg/hを押すと、Home/Endを送信する",
  "rules": [
    {
      "description": "home // [fn]+[g]=[home]",
      "manipulators": [
        {
          "from": {
            "key_code": "g",
            "modifiers": { "mandatory": [ "fn" ] }
          },
          "to": [
            {"key_code": "home"}
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "home // [fn]+[h]=[end]",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": { "mandatory": [ "fn" ] }
          },
          "to": [
            {"key_code": "end"}
          ],
          "type": "basic"
        }
      ]
    }
  ]
}